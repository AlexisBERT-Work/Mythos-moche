{
  "name": "mythos-archive",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "tsx src/server.ts",
    "gen:auth": "npx prisma generate --schema=./prisma/schema_postgre.prisma",
    "gen:lore": "npx prisma generate --schema=./prisma/schema_mongodb.prisma",
    "gen:all": "npm run gen:auth && npm run gen:lore",
    "migrate:auth": "npx prisma migrate dev --schema=./prisma/schema_postgre.prisma",
    "push:lore": "npx prisma db push --schema=./prisma/schema_mongodb.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.1.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "nodemon": "^3.1.11",
    "prisma": "^7.2.0"
  }
}
